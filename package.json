{
  "name": "express-api",
  "version": "1.0.0",
  "description": "backend express application with postgres for users and items",
  "main": "server.js",
  "scripts": {
    "test": "jest --verbose --runInBand --testLocationInResults --setupFiles dotenv/config --passWithNoTests",
