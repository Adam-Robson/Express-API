  },
  "homepage": "https://github.com/Adam-Robson/express-api#readme",
  "devDependencies": {
    "@babel/core": "^7.22.10",
    "@babel/eslint-parser": "^7.22.10",
    "@babel/eslint-plugin": "^7.22.10",
    "@babel/plugin-syntax-class-properties": "^7.12.13",
    "@types/jest": "^29.5.3",
    "@types/pg": "^8.10.2",
    "eslint": "^8.47.0",
    "jest": "^29.6.2",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.2",
    "supertest": "^6.3.3"
  },
